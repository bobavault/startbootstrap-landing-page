doctype html
html(lang='en')

    head

        meta(charset='utf-8')
        meta(name='viewport', content='width=device-width, initial-scale=1, shrink-to-fit=no')
        meta(name='description', content='Mint your own Boba Tea NFT and gain exclusive access to future NFT drops (e.g. Boba Monsters), passive earnings model, and future dApps.')
        meta(name='author', content='Boba Vault')

        title Boba Vault | Boba Tea NFT #{'&'} Boba Monsters

        // Favicon
        link(rel='icon', type='image/x-icon', href='assets/favicon.ico')

        // Bootstrap icons
        link(href='https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css', rel='stylesheet', type='text/css')

        // Google fonts
        link(href='https://fonts.googleapis.com/css?family=Lato:300,400,700,300italic,400italic,700italic', rel='stylesheet', type='text/css')

        // Core theme CSS (includes Bootstrap)
        link(href='css/styles.css', rel='stylesheet')

        // Global site tag (gtag.js) - Google Analytics
        script(async='' src='https://www.googletagmanager.com/gtag/js?id=G-Q9SCTJ3PXC')
        script.
          window.dataLayer = window.dataLayer || [];
          function gtag(){dataLayer.push(arguments);}
          gtag('js', new Date());
          gtag('config', 'G-Q9SCTJ3PXC');

    body

        // Navigation
        nav.navbar.navbar-expand-lg.navbar-light.bg-light
          .container-fluid
            a.navbar-brand(href='#') &#x1F9CB; Boba Vault
            button.navbar-toggler(type='button' data-bs-toggle='collapse' data-bs-target='#navbarTogglerDemo02' aria-controls='navbarTogglerDemo02' aria-expanded='false' aria-label='Toggle navigation')
              span.navbar-toggler-icon
            #navbarTogglerDemo02.collapse.navbar-collapse
              ul.navbar-nav.me-auto.mb-2.mb-lg-0
              .d-flex.gap-2
                a.btn.btn-outline-dark(href='https://twitter.com/bobavault')
                  i.bi.bi-twitter
                a.btn.btn-outline-dark(href='https://discord.gg/QBQdUverCJ')
                  i.bi.bi-discord
                a.btn.btn-primary(href='#!') Connect Wallet

        // Masthead
        header#mint.masthead
            .container.position-relative
                .row.justify-content-center.gap-5.align-items-center
                    .col-xl-5.text-white
                        // Page heading
                        h1.mb-4 10,000 unique Boba Teas!
                        p.mb-5.lead We're building an exclusive Boba Vault and your Boba Tea NFTs are the ticket! You'll get exclusive access to future NFT drops #{'('}e.g. BobaMonsters#{')'}, passive earnings model, and future dApps.
                        // Minting button
                        .row
                            .d-grid.gap-3.d-md-flex.justify-content-md-left
                                a.btn.btn-outline-light.btn-lg(href='#roadmap') Roadmap
                                button.btn.btn-primary.btn-lg(type='button' href='#!') MINT
                    .col-xl-5
                        #carousel.carousel.slide.carousel-fade(data-bs-ride='carousel' data-bs-pause='false' data-bs-interval='1000')
                            .carousel-inner 
                                .carousel-item.active.carousel-item-start
                                    img.d-block.w-100.rounded-3(src='/assets/img/carousel/tinified-2/01.png' alt='...')
                                .carousel-item
                                    img.d-block.w-100.rounded-3(src='/assets/img/carousel/tinified-2/02.png' alt='...')
                                .carousel-item
                                    img.d-block.w-100.rounded-3(src='/assets/img/carousel/tinified-2/03.png' alt='...')
                                .carousel-item
                                    img.d-block.w-100.rounded-3(src='/assets/img/carousel/tinified-2/04.png' alt='...') 
                                .carousel-item
                                    img.d-block.w-100.rounded-3(src='/assets/img/carousel/tinified-2/05.png' alt='...')
                                .carousel-item
                                    img.d-block.w-100.rounded-3(src='/assets/img/carousel/tinified-2/06.png' alt='...')
                                .carousel-item
                                    img.d-block.w-100.rounded-3(src='/assets/img/carousel/tinified-2/07.png' alt='...') 
                                .carousel-item
                                    img.d-block.w-100.rounded-3(src='/assets/img/carousel/tinified-2/08.png' alt='...')
                                .carousel-item
                                    img.d-block.w-100.rounded-3(src='/assets/img/carousel/tinified-2/09.png' alt='...') 

        // Varieties
        section.varieties.bg-light.text-center
            .container.pt-5.pb-5
                .row.pb-5.pt-4
                    .col
                        h2 Each Boba is unique, which one will you mint?
                .row.align-items-center
                    .col-lg-3.col-md-6.col-sm-6.col-6
                        img.d-block.w-100.rounded-3.mr-4.mb-4(src='/assets/img/varieties/tinified/02.png' alt='...')
                    .col-lg-3.col-md-6.col-sm-6.col-6
                        img.d-block.w-100.rounded-3.mr-4.mb-4(src='/assets/img/varieties/tinified/01.png' alt='...')
                    .col-lg-3.col-md-6.col-sm-6.col-6
                        img.d-block.w-100.rounded-3.mr-4.mb-4(src='/assets/img/varieties/tinified/12.png' alt='...')
