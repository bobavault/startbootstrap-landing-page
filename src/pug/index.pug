                        img.d-block.w-100.rounded-3.mr-4.mb-4(src='/assets/img/varieties/tinified/10.png' alt='...')
                .row.align-items-center
                    .col-lg-3.col-md-6.col-sm-6.col-6
                        img.d-block.w-100.rounded-3.mr-4.mb-4(src='/assets/img/varieties/tinified/03.png' alt='...')
                    .col-lg-3.col-md-6.col-sm-6.col-6
                        img.d-block.w-100.rounded-3.mr-4.mb-4(src='/assets/img/varieties/tinified/04.png' alt='...')
                    .col-lg-3.col-md-6.col-sm-6.col-6
